{{- $questionnaires := .Params.questionnaires }}
{{- $urlQuestionnaires := dict }}
{{- $questionnairesData := dict }}
{{- range $order, $questionnaireId := $questionnaires }}
    {{- $questionnaire := urls.JoinPath "questionnaires/" $questionnaireId | $.Site.GetPage }}
    {{- $urlQuestionnaires = $urlQuestionnaires | merge  (dict $questionnaireId (dict "questionnaireId" $questionnaireId "questionnaireUrl" $questionnaire.RelPermalink "order" $order )) }}
    {{- $questionnairesData = $questionnairesData | merge (dict $questionnaireId (dict "questionnaireId" $questionnaireId "questionnaireName" $questionnaire.Title "questionnaireLength" (len $questionnaire.Pages) "questionnaireShouldBeScored" $questionnaire.Params.shouldBeScored "order" $order ))}}
{{- end }}
{{- dict "batteryId" .Params.id "batteryName" .Title "questionnaires" $questionnairesData "urlQuestionnaires" $urlQuestionnaires | jsonify }}