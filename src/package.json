{
  "name": "testing-machine",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "dev": "hugo server -p 8000 --disableFastRender --ignoreCache --cleanDestinationDir",
    "dev-stats": "hugo server -p 8000 --disableFastRender --ignoreCache --cleanDestinationDir --templateMetrics --templateMetricsHints --printUnusedTemplates",
    "build-hugo": "hugo --gc --minify --cleanDestinationDir",
    "rm-app": "rm -r '../electron-prebuilt/app/public/'",
    "rm-mac": "rm -r '../electron-prebuilt/binaries/mac/Electron.app/Contents/Resources/app/'",
    "cp-mac": "cp -r '../electron-prebuilt/app' '../electron-prebuilt/binaries/mac/Electron.app/Contents/Resources'",
    "rm-win": "rm -r '../electron-prebuilt/binaries/win/electron/resources/app/'",
    "cp-win": "cp -r '../electron-prebuilt/app' '../electron-prebuilt/binaries/win/electron/resources'",
    "build": "npm run rm-app && npm run build-hugo && npm run rm-mac && npm run cp-mac && npm run rm-win && npm run cp-win"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.1",
    "alpinejs": "^3.14.1",
    "banditypes": "^0.2.5",
    "fflate": "^0.8.2",
    "htmx.org": "^2.0.1"
  }
}
